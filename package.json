{
  "name": "rockfordresiliencehub",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build:css": "cross-env NODE_ENV=production postcss src/bundle.css -o dist/bundle.css",
    "build:js": "terser src/script.js -c -m -o dist/script.js",
    "build": "npm run build:css && npm run build:js",
    "serve": "http-server -c-1 -p 8081"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.1.0",
    "cssnano": "^7.1.2",
    "html-minifier-terser": "^7.2.0",
    "http-server": "^14.1.1",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.0",
    "rollup": "^2.79.1",
    "tailwindcss": "^3.4.18",
    "terser": "^5.10.0"
  },
  "dependencies": {
    "rollup-plugin-terser": "^7.0.2"
  }
}
